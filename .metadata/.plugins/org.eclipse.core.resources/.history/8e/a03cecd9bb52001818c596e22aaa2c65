	@FindBy(xpath="//a[@title='Calendar']")
	WebElement calenderLink;
	
	@FindBy(xpath="//a[@title='Companies']")
	WebElement companyLink;
	
	public void homePage()
	{
		
		//homeLink.click();
		calenderLink.click();
	}

}
